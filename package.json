{
  "name": "digicam-ext",
  "version": "0.2.3",
  "private": true,
  "dependencies": {
    "@emotion/core": "^10.0.7",
    "@emotion/styled": "^10.0.7",
    "json2csv": "^4.5.1",
    "moment": "^2.24.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.5",
    "react-spring": "^8.0.23",
    "typescript": "^3.3.3"
  },
  "devDependencies": {
    "tslint": "^5.19.0",
    "@types/chrome": "^0.0.86",
    "@types/json2csv": "^4.5.0",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "@types/react-router-dom": "^4.3.1"
  },
  "scripts": {
    "dev": "react-scripts start",
    "build": "react-scripts build && tsc -t es6 --outDir build --moduleResolution node chrome/*.ts",
    "build/inject": "tsc chrome/*.ts && mv chrome/*.js build",
    "eject": "react-scripts eject",
    "zip": "zip -r digicam-ext.zip build/*",
    "lint": "tslint --project . --fix"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}