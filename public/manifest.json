{
  "name": "デジキャンヘルパー",
  "version": "0.1.1",
  "manifest_version": 2,
  "description": "自動化ツール for デジキャン",
  "homepage_url": "https://github.com/rainy-me/digicam-ext",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "options_page": "index.html#options",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "デジキャンヘルパー",
    "default_popup": "index.html"
  },
  "permissions": [
    "downloads",
    "storage",
    "tabs",
    "https://dh.force.com/*",
    "https://campus.dhw.ac.jp/*"
  ],
  "content_security_policy": "script-src 'self' 'sha256-5As4+3YpY62+l38PsxCEkjB1R4YtyktBtRScTJ3fyLU='; object-src 'self'"
}